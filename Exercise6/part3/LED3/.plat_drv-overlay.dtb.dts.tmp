/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2835", "brcm,bcm2836", "brcm,bcm2708", "brcm,bcm2709";

    fragment@0 {

        target-path = "/";
        __overlay__ {

            plat_drv: plat_drv@0 {

                compatible = "ase, led3";
                status = "okay";


                gpios = <&gpio 12 0>, <&gpio 16 0>, <&gpio 20 1>, <&gpio 21 1>, <&gpio 26 1>;



                mydevt-custom = <0x12345678>;
            };
        };
    };
};
